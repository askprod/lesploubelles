<p id="notice"><%= notice %></p>

<h1>Streets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @streets.each do |street| %>
      <tr>
        <td><%= link_to 'Show', city_street_path(street.city, street) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

